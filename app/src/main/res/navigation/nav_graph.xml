<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.guttatrener.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <argument
            android:name="dagNr"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="ukeNr"
            app:argType="string"
            app:nullable="true" />
        <action
            android:id="@+id/action_FirstFragment_to_login"
            app:destination="@id/login" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.guttatrener.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">
        <argument
            android:name="dagNr"
            app:argType="string" />
        <argument
            android:name="ukeNr"
            app:argType="string" />
        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/login"
        android:name="com.example.guttatrener.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
</navigation>